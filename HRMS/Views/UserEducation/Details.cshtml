@model HRMS.Models.UserEducation

@{
    ViewBag.Title = "Details";
    if (Ajax.IsThisAjaxRequest())
    {
        Layout = null;
    }
}
<section class="content-header">
    <h1>
        @ViewBag.Title
        <small>Details</small>
    </h1>
</section>
<section class="content">
    <!-- Small boxes (Stat box) -->
    <div class="row">
        <div class="col-xs-12">

            <div class="box box-solid box-primary">
                <div class="box-header">
                    <h3 class="box-title">Detail of UserEducation</h3>
                </div><!-- /.box-header -->
                <div class="box-body table-responsive">

                    <div class="widget-sec">
                        <div class="streaming-table">
                            <table class="table table-striped table-bordered">

                                <tr>
                                    <td>InstitutionId</td>
                                    <td>@Html.DisplayFor(model => model.InstitutionId)</td>
                                </tr>
                                <tr>
                                    <td>Degree</td>
                                    <td>@Html.DisplayFor(model => model.Digree)</td>
                                </tr>
                                <tr>
                                    <td>FromDate</td>
                                    <td>@Html.DisplayFor(model => model.FromDate)</td>
                                </tr>
                                <tr>
                                    <td>ToDate</td>
                                    <td>@Html.DisplayFor(model => model.ToDate)</td>
                                </tr>
                                <tr>
                                    <td>UserId</td>
                                    <td>@Html.DisplayFor(model => model.UserId)</td>
                                </tr>
                                <tr>
                                    <td>TotalMarks</td>
                                    <td>@Html.DisplayFor(model => model.TotalMarks)</td>
                                </tr>
                                <tr>
                                    <td>OutOfMarks</td>
                                    <td>@Html.DisplayFor(model => model.OutOfMarks)</td>
                                </tr>
                                <tr>
                                    <td>Percentage</td>
                                    <td>@Html.DisplayFor(model => model.Percentage)</td>
                                </tr>
                                <tr>
                                    <td>Last Salary</td>
                                    <td>@Html.DisplayFor(model => model.InstitutionName)</td>
                                </tr>
                                <tr>
                                    <td>Job Duration</td>
                                    <td>@Html.DisplayFor(model => model.LatestDegree)</td>
                                </tr>
                                <tr>
                                    <td>Reason For Leave</td>
                                    <td>@Html.DisplayFor(model => model.DivisionOrGrade)</td>
                                </tr>

 
                            </table>
                            <div class="box-footer">
                                @Ajax.ModalDialogActionLink("Edit", "Edit/" + Model.Id, "Edit", "btn btn-primary") 
                                 
                                @Html.ActionLink("Back to List", "Index", null, new { @class = "btn btn-primary" })
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>



