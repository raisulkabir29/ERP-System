@model HRMS.Models.ShiftMaster

@{
    /**/

    ViewBag.Title = "Are you sure you want to delete";
    if (Ajax.IsThisAjaxRequest())
    {
        Layout = null;
    }
}


@Html.StarkAjaxFormSubmiter("frmShiftMaster", "tbShiftMaster", true, "Action Successfully Executed")

@{
    if (!Ajax.IsThisAjaxRequest())
    {
        <section class="content-header">
            <h1>
                @ViewBag.Title
                @*<small>@ViewBag.Title</small>*@
            </h1>
        </section>
    }
}

<section class="content">
    <!-- Small boxes (Stat box) -->
    <div class="row">
        <div class="col-xs-12">

            <div class="box box-solid box-primary">
                <div class="box-header">
                    <h3 class="box-title">Detail of ShiftMaster</h3>
                </div><!-- /.box-header -->
                <div class="box-body table-responsive">

                    <div class="widget-sec">
                        <div class="streaming-table">
                            <table class="table table-striped table-bordered">

                                <tr>
                                    <td>OT/NOT</td>
                                    <td>@Html.DisplayFor(model => model.OTNOT)</td>
                                </tr>
                                <tr>
                                    <td>Code</td>
                                    <td>@Html.DisplayFor(model => model.Code)</td>
                                </tr>
                                <tr>
                                    <td>Name</td>
                                    <td>@Html.DisplayFor(model => model.Name)</td>
                                </tr>
                                <tr>
                                    <td>Description</td>
                                    <td>@Html.DisplayFor(model => model.Description)</td>
                                </tr>
                                <tr>
                                    <td>StartTime</td>
                                    <td>@Html.DisplayFor(model => model.StartTime)</td>
                                </tr>
                                <tr>
                                    <td>EndTime</td>
                                    <td>@Html.DisplayFor(model => model.EndTime)</td>
                                </tr>
                                <tr>
                                    <td>DateAdded</td>
                                    <td>@Html.DisplayFor(model => model.DateAdded)</td>
                                </tr>
                                <tr>
                                    <td>ModifiedDate</td>
                                    <td>@Html.DisplayFor(model => model.ModifiedDate)</td>
                                </tr>

                            </table>
                            <div class="box-footer">

                                @using (Html.BeginForm("Delete", "ShiftMaster", FormMethod.Post, new { id = "frmShiftMaster" }))
                                {
                                    @Html.AntiForgeryToken()

                                    @Html.StarkInputButtion("Delete", "submit", "btn btn-danger", "")
                                    @Html.ActionLink("Back to List", "Index", null, new { @class = "btn btn-primary", @style = "margin-left:5px;" })
                                }
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>