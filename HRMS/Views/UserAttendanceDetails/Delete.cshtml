@model HRMS.Models.UserAttendanceDetails

@{
    ViewBag.Title = "Are you sure you want to delete";
    if (Ajax.IsThisAjaxRequest())
    {
        Layout = null;
    }
}


@Html.StarkAjaxFormSubmiter("frmUserAttendanceDetails", "tbUserAttendanceDetails", true, "Action Successfully Executed")

@{
    if (!Ajax.IsThisAjaxRequest())
    {
        <section class="content-header">
            <h1>
                @ViewBag.Title
                @*<small>@ViewBag.Title</small>*@
            </h1>
        </section>
    }
}

<section class="content">
    <!-- Small boxes (Stat box) -->
    <div class="row">
        <div class="col-xs-12">

            <div class="box box-solid box-primary">
                <div class="box-header">
                    <h3 class="box-title">Detail of User Attendance Details</h3>
                </div><!-- /.box-header -->
                <div class="box-body table-responsive">

                    <div class="widget-sec">
                        <div class="streaming-table">
                            <table class="table table-striped table-bordered">

                                <tr>
                                    <td>UserId</td>
                                    <td>@Html.DisplayFor(model => model.UserId)</td>
                                </tr>
                                <tr>
                                    <td>UserAttndStatusId</td>
                                    <td>@Html.DisplayFor(model => model.UserAttndStatusId)</td>
                                </tr>
                                <tr>
                                    <td>PresentDate</td>
                                    <td>@Html.DisplayFor(model => model.PresentDate)</td>
                                </tr>
                                <tr>
                                    <td>Year</td>
                                    <td>@Html.DisplayFor(model => model.Year)</td>
                                </tr>
                                <tr>
                                    <td>Month</td>
                                    <td>@Html.DisplayFor(model => model.Month)</td>
                                </tr>
                                <tr>
                                    <td>CalendarDay</td>
                                    <td>@Html.DisplayFor(model => model.CalendarDay)</td>
                                </tr>
                                <tr>
                                    <td>PresentDate</td>
                                    <td>@Html.DisplayFor(model => model.TotalHoliDays)</td>
                                </tr>
                                <tr>
                                    <td>TotalWorkingDays</td>
                                    <td>@Html.DisplayFor(model => model.TotalWorkingDays)</td>
                                </tr>
                                <tr>
                                    <td>OvertimeHour</td>
                                    <td>@Html.DisplayFor(model => model.OvertimeHour)</td>
                                </tr>
                                <tr>
                                    <td>PreparedBy</td>
                                    <td>@Html.DisplayFor(model => model.PreparedBy)</td>
                                </tr>
                                <tr>
                                    <td>CheckedBy</td>
                                    <td>@Html.DisplayFor(model => model.CheckedBy)</td>
                                </tr>
                                <tr>
                                    <td>AuthorizedBy</td>
                                    <td>@Html.DisplayFor(model => model.AuthorizedBy)</td>
                                </tr>


                            </table>
                            <div class="box-footer">

                                @using (Html.BeginForm("Delete", "UserAttendanceDetails", FormMethod.Post, new { id = "frmRoleUser" }))
                                {
                                    @Html.AntiForgeryToken()

                                    @Html.StarkInputButtion("Delete", "submit", "btn btn-danger", "")
                                    @Html.ActionLink("Back to List", "Index", null, new { @class = "btn btn-primary", @style = "margin-left:5px;" })
                                }
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>
