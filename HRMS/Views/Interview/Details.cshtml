@model HRMS.Models.Interview

@{
    ViewBag.Title = "Details";
    if (Ajax.IsThisAjaxRequest())
    {
        Layout = null;
    }
}
<section class="content-header">
    <h1>
        @ViewBag.Title
        <small>Details</small>
    </h1>
</section>
<section class="content">
    <!-- Small boxes (Stat box) -->
    <div class="row">
        <div class="col-xs-12">

            <div class="box box-solid box-primary">
                <div class="box-header">
                    <h3 class="box-title">Detail of Interview</h3>
                </div><!-- /.box-header -->
                <div class="box-body table-responsive">

                    <div class="widget-sec">
                        <div class="streaming-table">
                            <table class="table table-striped table-bordered">

                                <tr>
                                    <td>CandidateName</td>
                                    <td>@Html.DisplayFor(model => model.CandidateName)</td>
                                </tr>
                                <tr>
                                    <td>Email</td>
                                    <td>@Html.DisplayFor(model => model.Email)</td>
                                </tr>
                                <tr>
                                    <td>Mobile</td>
                                    <td>@Html.DisplayFor(model => model.Mobile)</td>
                                </tr>
                                <tr>
                                    <td>Address</td>
                                    <td>@Html.DisplayFor(model => model.Address)</td>
                                </tr>
                                <tr>
                                    <td>JobTitleId</td>
                                    <td>@Html.DisplayFor(model => model.JobTitleId)</td>
                                </tr>
                                <tr>
                                    <td>InterviewDate</td>
                                    <td>@Html.DisplayFor(model => model.InterviewDate)</td>
                                </tr>
                                <tr>
                                    <td>PlaceOfInterview</td>
                                    <td>@Html.DisplayFor(model => model.PlaceOfInterview)</td>
                                </tr>
                                <tr>
                                    <td>InterviewTime</td>
                                    <td>@Html.DisplayFor(model => model.InterviewTime)</td>
                                </tr>
                                <tr>
                                    <td>InterviewBy</td>
                                    <td>@Html.DisplayFor(model => model.InterviewBy)</td>
                                </tr>
                                <tr>
                                    <td>DateAdded</td>
                                    <td>@Html.DisplayFor(model => model.DateAdded)</td>
                                </tr>
                                <tr>
                                    <td>AddedBy</td>
                                    <td>@Html.DisplayFor(model => model.AddedBy)</td>
                                </tr>
                                <tr>
                                    <td>ModifiedDate</td>
                                    <td>@Html.DisplayFor(model => model.ModifiedDate)</td>
                                </tr>
                                <tr>
                                    <td> InterviewRemarks</td>
                                    <td>  @Html.Raw(Model.InterviewRemarks)</td>
                                </tr>
                                <tr>
                                    <td>IsDone</td>
                                    <td>@Html.DisplayFor(model => model.IsDone)</td>
                                </tr>

 
                            </table>
                            <div class="box-footer">
                                @Ajax.ModalDialogActionLink("Edit", "Edit/" + Model.Id, "Edit", "btn btn-primary") 
                                 
                                @Html.ActionLink("Back to List", "Index", null, new { @class = "btn btn-primary" })
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>



