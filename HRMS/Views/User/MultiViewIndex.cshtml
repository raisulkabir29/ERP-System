                                                <div role="form">
                                                    <div class="box-body">
                                                        <div class="alert alert-success alert-dismissable" id="dvMsg" style="display:@(TempData["Display"]==null ? "none" : "")">
                                                            <i class="fa fa-apple"></i>
                                                            <b> Update Successfully. </b>
                                                        </div>


                                                        <div class="form-group">
                                                            @Html.LabelFor(model => model.UserName, new { @class = "" })
                                                            @Html.EditorFor(model => model.UserName)
                                                        </div>
                                                        <div class="form-group">
                                                            @Html.LabelFor(model => model.Password, new { @class = "" })
                                                            @Html.EditorFor(model => model.Password)
